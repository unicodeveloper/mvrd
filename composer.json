{
    "name": "unicodeveloper/mvrd",
    "description": "Unofficial Motor Vehicle Registration Information Search Portal Library",
    "keywords": ["php","api","motor","nigeria", "vehicle information", "registration"],
    "license": "MIT",
    "authors": [
      {
        "name": "unicodeveloper",
        "email": "prosperotemuyiwa@gmail.com"
      }
   ],
   "minimum-stability": "stable",
   "require": {
    "php" : "^5.4|^7.0",
    "guzzlehttp/guzzle": "5.*|6.*",
    "symfony/dom-crawler": "^3.2",
    "symfony/css-selector": "^2.8",
    "vlucas/phpdotenv": "^2.2"
   },
   "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1",
        "satooshi/php-coveralls": "^0.7.0",
        "mockery/mockery": "^0.9.4"
    },
   "autoload": {
        "psr-4": {
          "Unicodeveloper\\Mvrd\\": "src/"
        }
   },
   "autoload-dev": {
        "psr-4": {
            "Unicodeveloper\\Mvrd\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    }
}
